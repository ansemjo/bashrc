[push]
	default = simple
	followTags = true

[format]
	pretty = custom
[pretty]
	custom = format:%Cred%h %Cblue%ci%x08%x08%x08%x08%x08%x08%x08%x08%x08%x08%x08%x08%x08%x08%x08 (%cr)% GS %Creset%s

[alias]
	graph = log --graph --all
	summary = log --summary
  st = status
  co = checkout
  br = branch --verbose
  l = log -n 5
  d = diff

[color]
	ui = auto
